{
  "order" : 100,
  "index_patterns": [
    "*ReplaceWithYourIndexPatternHere*",
    "es_stk_test*"
  ],
  "version": 2022040401,
  "settings": {
    "analysis": {
      "analyzer": {
        "es_stk_analyzer": {
          "type": "custom",
          "char_filter": [ "whitespace_no_way" ],
          "filter": [ "trim", "lowercase", "max_token_truncate" ],
          "tokenizer": "keyword"
        }
      },
      "char_filter": {
        "whitespace_no_way": {
          "type": "pattern_replace",
          "pattern": "(\\s)+",
          "replacement": "$1"
        }
      },  
      "filter" : {
        "max_token_truncate": {
          "type": "truncate",
          "length": 32766
        },
        "path_hierarchy_pattern_filter": {
          "type" : "pattern_capture",
          "preserve_original": true,
          "patterns": [
            "((?:[^\\\\]*\\\\)*)(.*)",
            "((?:[^/]*/)*)(.*)"
          ]
        }
      },  
      "tokenizer": {
        "path_tokenizer": {
          "type": "path_hierarchy",
          "delimiter": "\\"
        }
      }
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "strings": {
          "match_mapping_type": "string",
          "mapping": {
            "type": "keyword",
            "ignore_above": 32765,
            "fields": {
              "security": {
                "type": "text",
                "analyzer": "es_stk_analyzer",
                "index_options": "docs",
                "norms": "false"
              }
            }
          }
        }
      }
  ],
  "properties": { }
  }
}
